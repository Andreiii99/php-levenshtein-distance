{
    "name": "jfcherng/php-levenshtein-distance",
    "description": "Calculate the Levenshtein distance and edit progresses between two strings.",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Jack Cherng",
            "email": "jfcherng@gmail.com"
        }
    ],
    "keywords": [
        "edit-progress",
        "levenshtein-distance"
    ],
    "autoload": {
        "psr-4": {
            "Jfcherng\\Diff\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Jfcherng\\Diff\\Test\\": "tests/"
        }
    },
    "config": {
        "sort-packages": true
    },
    "require": {
        "php": "^7.1.3"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^2.14.1",
        "phan/phan": "^1.2",
        "phpunit/phpunit": "^7.2 || ^8.0"
    },
    "scripts": {
        "analyze": [
            "phan --color"
        ],
        "fix": [
            "php-cs-fixer fix --verbose"
        ],
        "test": [
            "phpunit --verbose"
        ]
    }
}
