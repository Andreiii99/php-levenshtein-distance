{
    "name": "jfcherng/php-levenshtein-distance",
    "description": "Calculate the Levenshtein distance and edit progresses between two strings.",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Jack Cherng",
            "email": "jfcherng@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Jfcherng\\Utility\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Jfcherng\\Utility\\Test\\": "tests/"
        }
    },
    "config": {
        "sort-packages": true
    },
    "require": {
        "php": "^7.1.3"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^2.12.1",
        "phpunit/phpunit": "^7.2.6"
    },
    "scripts": {
        "fix": [
            "php-cs-fixer fix --verbose"
        ],
        "test": [
            "phpunit --verbose"
        ]
    }
}
